        },
        "Given_Name": {
          "type": "string"
        },
        "Nationality": {
          "type": "string"
        },
        "Sex": {
          "type": "string"
        },
        "Date_of_Birth": {
          "type": "string"
        },
        "Place_of_Birth": {
          "type": "string"
        },
        "Date_of_Issue": {
          "type": "string"
        },
        "Date_of_Expiry": {
          "type": "string"
        },
        "Place_of_Issue": {
          "type": "string"
        }
